{
    "name": "rowanism/terrorwave-web",
    "type": "project",
    "description": "Web GUI for the Lufia 2 Terrorwave Randomizer",
    "keywords": [
        "lufia2",
        "terrorwave",
        "randomizer",
        "laravel",
        "htmx",
        "hyperscript"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "abitofBaileys",
            "email": "abitof@baileys.games",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "abyssonym/terrorwave",
                "version": "dev-master",
                "dist": {
                    "url": "https://github.com/abyssonym/terrorwave/releases/latest/download/l2_terror_wave_source.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "phku-hk/terrorwave-blitz",
                "version": "master",
                "dist": {
                    "url": "https://github.com/phku-hk/terrorwave-blitz/releases/download/v1.0.1/source_full_v1.0.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": "^8.1",
        "abyssonym/terrorwave": "dev-master",
        "phku-hk/terrorwave-blitz": "master",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "mauricius/laravel-htmx": "^0.3.0",
        "spatie/laravel-directory-cleanup": "^1.9",
        "symfony/process": "^6.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
